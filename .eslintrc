{
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "linebreak-style": ["error", "windows"],
    "react/jsx-uses-vars": 1,
    "comma-dangle": ["error", {"functions": "ignore"}]
  },
  "extends": ["airbnb-base"],
  "env": {
    "browser": true,
    "node": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx"]
      }
    }
  }
}